    "#2942. Find Words Containing Character\n",
    "def findWordsContaining(words,x):\n",
    "    result = []\n",
    "    for w in (words):\n",
    "        if x in w:\n",
    "            result.append(words.index(w))\n",
    "    return result\n",
    "\n",
    "words = [\"leet\",\"code\"]\n",
    "x = \"e\"\n",
    "print(findWordsContaining(words,x))"
